<resources>

    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar" />

    <style name="title_style">
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@color/gray</item>
    </style>

    <style name="base_text_color_black">
        <item name="android:textColor">@color/setup_title_back_color</item>
    </style>

    <style name="title_style.title_text">
        <item name="android:background">@color/white</item>
        <item name="android:textSize">25dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/setup_title_back_color</item>
    </style>

    <style name="title_style.title_relative_text"></style>

    <style name="fragment_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="footer_style">
        <item name="android:background">@color/white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="footer_style.radio_group">
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_marginBottom">3dp</item>
        <item name="android:layout_marginTop">3dp</item>
    </style>

    <style name="footer_style.radio_button">
        <item name="android:button">@null</item>
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginLeft">13dp</item>
        <item name="android:layout_marginRight">13dp</item>
    </style>

    <style name="follow_button">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/selector_white</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="vertical_line">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:background">@color/contact_setting_items_content_normal_color</item>
    </style>

    <style name="follow_button_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/text_color_white_and_gray_selector</item>
        <item name="android:paddingLeft">10dp</item>
    </style>

    <style name="listview_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:paddingLeft">3dp</item>
        <item name="android:paddingRight">3dp</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:paddingBottom">3dp</item>
        <item name="android:background">@color/listview_divider_color2</item>
    </style>

    <style name="listview_style.first_linear">
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/selector_white</item>
    </style>

    <style name="listview_style.avatar">
        <item name="android:layout_width">45dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:background">@drawable/selector_white</item>
    </style>

    <style name="listview_style.second_linear">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:background">@drawable/selector_white</item>
    </style>

    <style name="frame_function">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:background">@drawable/selector_white</item>
        <item name="android:paddingLeft">10dp</item>
    </style>

    <style name="image_funciton">
        <item name="android:layout_width">24dp</item>
        <item name="android:layout_height">24dp</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="text_function">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_marginLeft">30dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/setup_title_back_color</item>
    </style>

    <style name="repo_text_style">
        <item name="android:textColor">@color/setup_title_back_color</item>
    </style>

    <style name="repo_state_text_style" parent="repo_text_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:maxLines">1</item>
        <item name="android:paddingLeft">3dp</item>
    </style>

    <style name="repo_text_style.language">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="repo_state_text_style.star">
        <item name="android:layout_weight">1.5</item>
    </style>

    <style name="repo_state_text_style.fork">
        <item name="android:layout_weight">1.5</item>
    </style>

    <style name="repo_text_style.repo_name">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:maxLines">1</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="repo_text_style.description">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:minHeight">10dp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="repo_state_detail_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/selector_white</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="repo_state_detail_img_style">
        <item name="android:layout_width">24dp</item>
        <item name="android:layout_height">24dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="repo_state_detail_text_style" parent="base_text_color_black">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="base_line_color">
        <item name="android:background">@color/contact_setting_items_content_normal_color</item>
    </style>

    <style name="horizontal_line" parent="base_line_color">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
    </style>

    <style name="repo_state_detail_img_forward_style">
        <item name="android:layout_width">24dp</item>
        <item name="android:layout_height">24dp</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="repo_state_detail_relative_style">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/selector_white</item>
        <item name="android:orientation">horizontal</item>
    </style>

</resources>
